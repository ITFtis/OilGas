
<iframe src="@(ViewBag.PageUrl)"></iframe>
<style>
    .body-content {
        width: 100% !important;
        height: 100%;
        padding: 0;
        height: calc( 100% - var(--dou-menu-height) - 1px ) !important;
        min-height:auto !important;
    }
    .body-content > iframe:first-child {
        width: 100%;
        height:100%;
    }
</style>
<script>
    helper.misc.showBusyIndicator();
    $('.body-content > iframe:first-child').on("load",function () {
        helper.misc.hideBusyIndicator();
    });
</script>

